@{
	ViewData["Title"] = "ViewTeacherCourse";
}
<div class="row">
	<div class="col-md-4">
		<form asp-action="FindTeacherCourse">
			<div class="form-group">
				<label class="control-label">Select course</label>
				<select class="form-control" name="id" asp-items="ViewBag.CourseId"></select>
			</div>
			<div class="form-group">
				<input type="submit" value="FindTeacherCourse" class="btn btn-primary" />
			</div>
		</form>
	</div>
</div>